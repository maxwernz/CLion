# eine Konstante
p1: 17

# eine einfache lineare Funktion
p2: 1 0

# eine einfache quadratische Funktion
p3: 1 0 0

# eine komplexere quadratische Funktion
a1: 1.2 3.4 5.6

echo alle Polynome
dump 

echo Werte p1 aus
show p1
eval p1 1 2 3 
echo Werte p2 aus
show p2
eval p2 1 2 3 
echo Werte p3 aus
show p3
eval p3 1 2 3 
echo Werte a1 aus
show a1
eval a1 1 2 3 

echo Loesche p1 und p2 und dumpe
del p1 p2
dump

echo nach einem Leeren hat der finale Dump keinen Effekt mehr
clear